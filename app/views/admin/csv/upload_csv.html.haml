= form_for :dump, :url => { :action => "import_csv" }, :html => {:multipart => true } do |f|
	%table
		%tr
			%td
				%label{:for => "dump_file"}
					Select a CSV File:
			%td
				= f.file_field :file
		%tr
			%td
				= submit_tag "Save changes", :id => "submit", :name => "submit", :class => "form_submit", :disabled => false, :disable_with => "Please wait..."